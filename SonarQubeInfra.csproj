<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <AssemblyName>SonarQube</AssemblyName>
    <RootNamespace>SonarQube</RootNamespace>
    <Product>Sonarq</Product>
    <LangVersion>latestmajor</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Pulumi" Version="3.*" />
    <PackageReference Include="Pulumi.Kubernetes" Version="3.*" />
    <PackageReference Include="Pulumi.PostgreSql" Version="3.5.0" />
  </ItemGroup>

</Project>
